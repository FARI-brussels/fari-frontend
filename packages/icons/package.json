{
  "name": "@fari/icons",
  "version": "0.1.0",
  "description": "Minimal SVG icons for Fari projects",
  "main": "dist/index.js",
  "exports": {
  ".": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  }
},
"types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build": "node src/scripts/generateIconTypes.mjs && vite build && pnpm run build:types",
    "dev": "node src/scripts/generateIconTypes.mjs && vite build --watch",
    "generate-types": "node src/scripts/generateIconTypes.mjs",
    "optimize": "svgo -f src -o src"
  },
  "dependencies": {
    "vite-svg-loader": "^5.1.0"
  },
  "devDependencies": {
    "svgo": "^3.0.0",
    "vite": "^6.2.2",
    "typescript": "^5.0.0"
  
  }
}